class Solution:
    # Turn list to int, add, return list version of int(str(x))
    def plusOne(self, digits: list[int]) -> list[int]:
        s = ""
        for i in range(len(digits)):
            s += str(digits[i])
        inty = str(int(s) + 1)
        res = []
        for i in range(len(inty)):
            res.append(int(inty[i]))
        
        return res 
        
        